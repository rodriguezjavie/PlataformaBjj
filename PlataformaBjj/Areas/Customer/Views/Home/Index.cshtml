<!--================ RELATED POSTS ================-->
<section class="s-related-posts home-related-posts" id="noticias">
    <div class="container">
        <h2 class="title-decor">Ultimas <span>Noticias</span></h2>
        <p class="slogan">Enterate de nuestras ultimas noticias y acontecimientos, los iremos relatando en esta sección.</p>
        <div class="row ">
            @*@{int i = -1; }*@
            @foreach (var item in Model)
            {


                <div class="col-md-6 related-post-col">
                    <div class="post-item-cover">
                        <div class="post-header">
                            <div class="related-post-categ">@item.Type</div>
                            <div class="post-thumbnail">
                                <a asp-area="Customer" asp-action="Details" asp-controller="News" asp-route-id="@item.Id"><img class="rx-lazy" src="@item.Image" data-src="@item.Image" alt="img"></a>
                            </div>
                        </div>
                        <div class="post-content">
                            <div class="meta">
                                <span class="post-date"><i class="fa fa-calendar" aria-hidden="true"></i>@item.UploadDate</span>
                            </div>
                            <h3 class="title"><a asp-area="Customer" asp-action="Details" asp-controller="News" asp-route-id="@item.Id">@item.Title</a></h3>
                            <div class="text">
                                <p>@Html.Raw(item.Description.Substring(0, 200)) ...</p>
                            </div>
                        </div>
                        <div class="post-footer">
                            <a asp-area="Customer" asp-action="Details" asp-controller="News" asp-route-id="@item.Id" class="btn btn-danger"><span>leer más</span></a>
                        </div>
                    </div>
                </div>
                @*if (i % 2 == 0)
                {
                    <div class="w-100"></div>
                    <br />
                }
                i++;*@
            }
        </div>
    </div>
</section>
<!--============== RELATED POSTS END ==============-->
